<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tool="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/time_item_hour_one_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/home_fragment_shape_bg_radius_2_f4f3f3"
        android:gravity="center"
        android:paddingStart="@dimen/size_5dp"
        android:paddingEnd="@dimen/size_5dp"
        android:textColor="@color/chn_color__black_1A1B1A"
        android:textSize="@dimen/sp_19"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tool:text="9" />


    <TextView
        android:id="@+id/time_item_hour_two_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/home_fragment_shape_bg_radius_2_f4f3f3"
        android:gravity="center"
        android:paddingStart="@dimen/size_5dp"
        android:paddingEnd="@dimen/size_5dp"
        android:textColor="@color/chn_color__black_1A1B1A"
        android:textSize="@dimen/sp_19"
        app:layout_constraintLeft_toRightOf="@id/time_item_hour_one_tv"
        app:layout_constraintTop_toTopOf="parent"
        tool:text="9" />

    <TextView
        android:id="@+id/time_item_hint_one_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/size_3dp"
        android:gravity="center"
        android:text=":"
        android:textColor="@color/chn_color__black_1A1B1A"
        android:textSize="@dimen/sp_19"
        app:layout_constraintLeft_toRightOf="@id/time_item_hour_two_tv"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/time_item_minute_one_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/home_fragment_shape_bg_radius_2_f4f3f3"
        android:gravity="center"
        android:paddingStart="@dimen/size_5dp"
        android:paddingEnd="@dimen/size_5dp"
        android:textColor="@color/chn_color__black_1A1B1A"
        android:textSize="@dimen/sp_19"
        app:layout_constraintLeft_toRightOf="@id/time_item_hint_one_tv"
        app:layout_constraintTop_toTopOf="parent"
        tool:text="9" />

    <TextView
        android:id="@+id/time_item_minute_two_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/home_fragment_shape_bg_radius_2_f4f3f3"
        android:gravity="center"
        android:paddingStart="@dimen/size_5dp"
        android:paddingEnd="@dimen/size_5dp"
        android:textColor="@color/chn_color__black_1A1B1A"
        android:textSize="@dimen/sp_19"
        app:layout_constraintLeft_toRightOf="@id/time_item_minute_one_tv"
        app:layout_constraintTop_toTopOf="parent"
        tool:text="9" />

    <TextView
        android:id="@+id/time_item_hint_two_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/size_3dp"
        android:gravity="center"
        android:text=":"
        android:textColor="@color/chn_color__black_1A1B1A"
        android:textSize="@dimen/sp_19"
        app:layout_constraintLeft_toRightOf="@id/time_item_minute_two_tv"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/time_item_second_one_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/home_fragment_shape_bg_radius_2_f4f3f3"
        android:gravity="center"
        android:paddingStart="@dimen/size_5dp"
        android:paddingEnd="@dimen/size_5dp"
        android:textColor="@color/chn_color__black_1A1B1A"
        android:textSize="@dimen/sp_19"
        app:layout_constraintLeft_toRightOf="@id/time_item_hint_two_tv"
        app:layout_constraintTop_toTopOf="parent"
        tool:text="9" />

    <TextView
        android:id="@+id/time_item_second_two_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/home_fragment_shape_bg_radius_2_f4f3f3"
        android:gravity="center"
        android:paddingStart="@dimen/size_5dp"
        android:paddingEnd="@dimen/size_5dp"
        android:textColor="@color/chn_color__black_1A1B1A"
        android:textSize="@dimen/sp_19"
        app:layout_constraintLeft_toRightOf="@id/time_item_second_one_tv"
        app:layout_constraintTop_toTopOf="parent"
        tool:text="9" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/time_group"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"
        app:constraint_referenced_ids="time_item_hour_one_tv,time_item_hour_two_tv,time_item_hint_one_tv,time_item_minute_one_tv,time_item_minute_two_tv,time_item_hint_two_tv,time_item_second_one_tv,time_item_second_two_tv" />

    <TextView
        android:id="@+id/time_item_date_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/chn_color__black_1A1B1A"
        android:textSize="@dimen/sp_19"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tool:text="12å¤©" />
</androidx.constraintlayout.widget.ConstraintLayout>